{
	"defaultSeverity": "warning",
	"linterOptions": {
		"exclude": [
			"node_modules/**",
			"lib/**",
			"dist/**"
		]
	},
	"rules": {
		"quotemark": [true, "single"],
		"indent": [true, "tabs", 4],
		"interface-name": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"no-consecutive-blank-lines": false,
		"semicolon": false,
		"arrow-parens": false,
		"trailing-comma": false,
		"curly": false,
		"no-console": false,
		"whitespace": false
	}
}
